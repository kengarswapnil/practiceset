class Solution {
    public int[] createTargetArray(int[] nums, int[] index) {
        int res[] = new int[nums.length];
        int s = 0;

        for(int i=0;i<nums.length;i++){
          for(int j = s;j>index[i];j--){
            res[j] = res[j-1];
          }
          res[index[i]] = nums[i];
          s++;
        }
        return res;
    }
}
